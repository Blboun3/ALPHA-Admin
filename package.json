{
  "name": "alpha-admin",
  "version": "5.0.0",
  "versionName": "Ennio Morricone",
  "description": "Discord admin bot made for ALPHA server",
  "main": "./dist/app.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "deploy-commands": "ts-node scripts/deploy_commands.ts",
    "dev": "nodemon -w ./src ./src/app.ts",
    "build": "tsc -p ./tsconfig.build.json",
    "start": "node ./dist/app.js",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts . --fix",
    "format": "prettier --write --ignore-unknown .",
    "prepare": "husky install",
    "generate": "prisma generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blboun3/ALPHA-Admin.git"
  },
  "author": "Blboun3",
  "license": "MIT-0",
  "bugs": {
    "url": "https://github.com/Blboun3/ALPHA-Admin/issues"
  },
  "homepage": "https://github.com/Blboun3/ALPHA-Admin#readme",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.39",
    "@prisma/client": "^4.12.0",
    "discord.js": "^14.9.0",
    "ms": "^2.1.3",
    "pino": "^8.11.0",
    "text-table": "^0.2.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.1",
    "@tsconfig/strictest": "^2.0.0",
    "@types/ms": "^0.7.31",
    "@types/text-table": "^0.2.2",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "eslint": "8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-error": "^1.0.2",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "nodemon": "^2.0.22",
    "prettier": "2.8.7",
    "prisma": "^4.12.0",
    "ts-node": "github:TypeStrong/ts-node#main",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "**/*.ts": "eslint",
    "**/*": "prettier --write --ignore-unknown"
  }
}
